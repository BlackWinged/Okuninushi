<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Inno hostels</title>



    <!-- Library CSS -->
    <link href="/css/scala_library.css" rel="stylesheet">

    <!-- Icons CSS -->
    <link href="/fonts/themify-icons.css" rel="stylesheet">

    <!-- Theme CSS -->
    <link href="/css/scala_style.css" rel="stylesheet">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Dosis:300,400,600,700%7COpen+Sans:300,400,700%7CPlayfair+Display:400,400i,700,700i" rel="stylesheet">

</head>


<body class="sca_middle_titles">

    <!-- passpartu -->
    <div class="sca_border_top hidden-sm hidden-xs"></div>
    <div class="sca_border_bottom hidden-sm hidden-xs"></div>
    <div class="sca_border_left hidden-sm hidden-xs"></div>
    <div class="sca_border_right hidden-sm hidden-xs"></div>
    <!-- passpartu end-->
    <!-- Preloader -->
    <div class="sca_page_loader sca_image_bck sca_wht_txt" data-color="#282828">
        <div class="sca_loader">
            <p><img src="~/images/logos/Inno_white.png" class="sca_img_shadow" height="150" alt=""></p>
            <div class="sca_spinner">
                <div class="sca_rect1"></div>
                <div class="sca_rect2"></div>
                <div class="sca_rect3"></div>
                <div class="sca_rect4"></div>
                <div class="sca_rect5"></div>
            </div>
        </div>
    </div>
    <!-- Preloader End-->
    <!-- Page -->
    <div class="sca_page" id="sca_page">
        <!-- To Top -->
        <a href="#sca_page" class="sca_top ti ti-angle-up sca_go"></a>

        <!-- Header -->
        <header>



            <nav class="sca_light_nav sca_transp_nav">
                <div class="container">

                    <div class="sca_logo_container clearfix">
                        <img src="~/images/logos/Inno_white_basic.png" alt="" class="sca_logo_rev">
                        <div class="sca_logo_txt">
                            <!-- Logo -->
                            <a href="@Url.Action("Index")" class="sca_logo">Inno Hostel</a>

                            <!-- Text Logo -->
                            <div class="sca_logo_und">Hostel & Pub lounge</div>
                        </div>
                    </div>


                    <!-- Menu -->
                    <div class="sca_main_menu sca_main_menu_mobile">

                        <div class="sca_main_menu_icon">
                            <i></i><i></i><i></i><i></i>
                            <b>Menu</b>
                            <b class="sca_main_menu_icon_b">Back</b>
                        </div>
                    </div>

                    <!-- Menu Content -->
                    <div class="sca_main_menu_content sca_image_bck" data-color="rgba(0,0,0,0.9)" data-image="/images/hotel/2-cvrzdeknw-alexander-videnov.jpg">
                        <!-- Over -->
                        <div class="sca_over" data-color="#000" data-opacity="0.7"></div>
                    </div>

                    <div class="sca_main_menu_content_menu sca_wht_txt text-right">
                        <div class="container">
                            <ul>
                                <li class="sca_parent">
                                    <a href="javascript:void(0)">Pages</a>
                                    <ul>
                                        <li><a href="@Url.Action("About1")">About 1</a></li>
                                        <li><a href="@Url.Action("About2")">About 2</a></li>
                                        <li><a href="@Url.Action("About3")">About 3</a></li>
                                        <li><a href="@Url.Action("Contacts")">Contacts</a></li>
                                    </ul>
                                </li>

                                <li class="sca_parent">
                                    <a href="#">Blog</a>
                                    <ul>
                                        <li><a href="@Url.Action("Blogsidebar")">Blog with Sidebar</a></li>
                                        <li><a href="@Url.Action("Blogonecol")">Blog Full Width</a></li>
                                        <li><a href="@Url.Action("Blogsingleside")">Single Post with Sidebar</a></li>
                                        <li><a href="@Url.Action("Blogsinglecol")">Single Post Full Width</a></li>
                                    </ul>
                                </li>


                            </ul>
                            <div class="sca_main_menu_content_menu_copy">
                                <form>
                                    <input type="text" class="form-control" placeholder="Enter Your Keywords">
                                    <button type="submit" class="btn">
                                        Search
                                    </button>
                                </form>
                                <!--<p>© ScalaThemes 2017</p>
                                <div class="sca_footer_social">
                                    <a href="#"><i class="ti ti-facebook sca_icon_box"></i></a>
                                    <a href="#"><i class="ti ti-instagram sca_icon_box"></i></a>
                                    <a href="#"><i class="ti ti-google sca_icon_box"></i></a>
                                    <a href="#"><i class="ti ti-youtube sca_icon_box"></i></a>
                                    <a href="#"><i class="ti ti-twitter sca_icon_box"></i></a>
                                    <a href="#"><i class="ti ti-pinterest sca_icon_box"></i></a>
                                </div> -->

                            </div>
                        </div>
                        <!-- container end -->
                    </div>
                    <!-- menu content end -->
                    <!-- Search Block -->
                    <div class="sca_search_block">
                        <div class="sca_basket_block_link sca_search_parent">
                            <i class="ti ti-shopping-cart-full"></i>
                            <ul>
                                <li class="sca_bask_menu">
                                    <a href="#" class="bask_block_item">
                                        <img src="/images/shop/g_mini1.jpg" alt="">
                                        GL551JM 15-Inch Gaming Laptop
                                        $320.20
                                    </a>

                                    <a href="#" class="bask_block_item">
                                        <img src="/images/shop/g_mini2.jpg" alt="">
                                        Encore 2 WT10-A32 10.0
                                        $110.40
                                    </a>
                                    <div class="sca_bask_btns">
                                        <span class="total">Total:$430.60</span>
                                        <a href="#" class="">Checkout</a>
                                    </div>
                                </li>
                            </ul>
                        </div>
                        <div class="sca_search_block_link sca_search_parent">
                            <i class="ti ti-search"></i>
                            <ul>
                                <li>
                                    <form>
                                        <input type="text" class="form-control" placeholder="Enter Your Keywords">
                                        <button type="submit" class="btn">
                                            <i class="ti ti-search"></i>
                                        </button>
                                    </form>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <!-- Search Block End -->
                    <!-- Top Menu -->
                    <div class="sca_default_menu">
                        <ul>
                            <li class="sca_parent">
                                <a href="javascript:void(0)">Pages</a>
                                <ul>
                                    <li><a href="@Url.Action("About1")">About 1</a></li>
                                    <li><a href="@Url.Action("About2")">About 2</a></li>
                                    <li><a href="@Url.Action("About3")">About 3</a></li>
                                    <li><a href="@Url.Action("Contacts")">Contacts</a></li>
                                </ul>
                            </li>

                            <li class="sca_parent">
                                <a href="#">Blog</a>
                                <ul>
                                    <li><a href="@Url.Action("Blogsidebar")">Blog with Sidebar</a></li>
                                    <li><a href="@Url.Action("Blogonecol")">Blog Full Width</a></li>
                                    <li><a href="@Url.Action("Blogsingleside")">Single Post with Sidebar</a></li>
                                    <li><a href="@Url.Action("Blogsinglecol")">Single Post Full Width</a></li>
                                </ul>
                            </li>

                        </ul>
                    </div>
                    <!-- Top Menu End -->

                </div>
                <!-- container end -->
            </nav>

        </header>
        <!-- Header End -->

        @RenderBody()

    </div>
    <!-- Page End -->
    <!-- JQuery -->
    <script src="/js/jquery-1.12.4.min.js"></script>
    <!-- Library JS -->
    <script src="/js/scala_library.js"></script>

    <!-- Theme JS -->
    <script src="/js/scala_script.js"></script>


</body>

